<div class="container">
  <h3>Users</h3>
  <table class="responsive-table">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Title</th>
        <th>Email</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% _.each(users, function (user) { %>
        <tr data-id="<%= user.id %>" data-model="user">
          <td><%= user.id %></td>
          <td><%= user.name %></td>
          <td><%= user.title %></td>
          <td><%= user.email %></td>
          <td style="text-align:right;"><a href="/user/show/<%= user.id %>" class="btn-floating blue"><i class="material-icons">visibility</i></a></td>
          <td style="text-align:center;"><a href="/user/edit/<%= user.id %>" class="btn-floating green"><i class="material-icons">edit</i></a></td>
          <td style="text-align:left;"><a href="/user/destroy/<%= user.id %>" class="btn-floating red"><i class="material-icons">delete</i></a></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>

<div class="container">
  <hr>
  <footer class="footer">
    <div class="right">
      <a href="http://sailsjs.com/">sails.js</a>
      <div>tutorial by GHEZAL Mohammed Amine</div>
    </div>
  </footer>
</div>